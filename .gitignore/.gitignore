#include <iostream>
using namespace std;
int main()
{
    float sueldo;
    cout<<"Ingresa tu sueldo actual ";
    cin>>sueldo;

    if (sueldo>=0 && sueldo<=1000)
    {
        sueldo=18*sueldo/100+sueldo;
    }
    else if (sueldo>=1001 && sueldo<=1100)
    {
        sueldo=15*sueldo/100+sueldo;
    }
    else if (sueldo>=1101 && sueldo<=1200)
    {
        sueldo=12*sueldo/100+sueldo;
    }
    else if (sueldo>=1201 && sueldo<=1300)
    {
        sueldo=10*sueldo/100+sueldo;
    }
    else if (sueldo>=1301)
    {
        sueldo=8*sueldo/100+sueldo;
    }
    else// if (sueldo<0)
    {
        cout<<"ERROR"<<endl;
    }

    cout<<"Tu nuevo sueldo es : "<<sueldo;

    return 0;
}
